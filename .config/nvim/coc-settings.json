{
  "volar.takeOverMode.enabled": false,
  "tsserver.enable": true,
  "snippets.priority": 1000,
  "snippets.userSnippetsDirectory": "~/.snippets",
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.virtualText": false,
  "prettier.semi": false,
  "prettier.trailingComma": "all",
  "tsserver.log": "verbose",
  "tsserver.trace.server": "verbose",
  "diagnostic.warningSign": ">>",
  "coc.preferences.extensionUpdateCheck": "never",
  "typescript.suggestionActions.enabled": false,
  "ruff.enableExperimentalFormatter": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "sql",
    "dart",
    "html",
    "json",
    "java",
    "typescriptreact",
    "go",
    "lua",
    "css",
    "c",
    "cs",
    "cpp",
    "python",
    "rust",
    "vue",
    "javascript",
    "typescript",
    "svelte",
    "yaml",
    "php"
  ],
  "go.goplsOptions": {
    "gofumpt": true
  },
  "eslint.enable": true,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "svelte"
  ],
  "eslint.validate": ["svelte", "vue"],
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "rust-analyzer.inlayHints.chainingHints": false,
  "python.linting.pylintEnabled": false,
  "python.linting.mypyEnabled": true,
  "python.linting.enabled": true,
  "python.pythonPath": "python3",
  "ruff.autoFixOnSave": true,
  "python.formatting.provider": "none",
  "suggest.noselect": false,
  "suggest.completionItemKindLabels": {
    "function": "\uf794",
    "method": "\uf6a6",
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\ufa52",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf668",
    "color": "\ue22b",
    "property": "\ufab6",
    "field": "\uf93d",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf893",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c"
  },
  "Lua.diagnostics.globals": ["vim"],
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp"],
      "rootPatterns": ["compile_commands.json", ".git/"],
      "initializationOptions": {
        "cacheDirectory": "/tmp/ccls"
      }
    },
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["cs"],
      "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
    },
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript"],
      "port": 6008
    },
    "golangci-lint-languageserver": {
      "command": "golangci-lint-langserver",
      "filetypes": ["godisabled"],
      "initializationOptions": {
        "command": [
          "golangci-lint",
          "run",
          "--out-format",
          "json",
          "--issues-exit-code=1"
        ]
      }
    }
  },
  "svelte.enable-ts-plugin": true,
  "snippets.ultisnips.pythonPrompt": false
}
